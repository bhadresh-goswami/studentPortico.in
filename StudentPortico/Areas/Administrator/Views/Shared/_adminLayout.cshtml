<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/adminLayout/css")

    @Scripts.Render("~/adminLayout/js")

   

</head>
<body>



    <div id="wrapper">
        <!-- Navigation -->

        @Html.Partial("_adminMenu")



        <div id="page-wrapper">
            <div class="container-fluid">

                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">@ViewBag.Title</h1>
                    </div>

                    <!-- /.col-lg-12 -->
                </div>

                @Html.Partial("_showMsgErr")
                @RenderBody()
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
    </div>





</body>
</html>
